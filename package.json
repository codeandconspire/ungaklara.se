{
  "name": "ungaklara.se",
  "version": "1.0.0",
  "description": "Official webpage of youth theatre Unga Klara",
  "main": "index.js",
  "scripts": {
    "build": "jalla index.js --sw sw.js --build",
    "start": "node server.js",
    "test": "standard"
  },
  "documentify": {
    "transform": [
      [
        "./lib/document.js",
        {
          "order": "end"
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeandconspire/ungaklara.se.git"
  },
  "now": {
    "alias": "ungaklara.conspire.online",
    "env": {
      "NODE_ENV": "staging",
      "CLOUDINARY_KEY": "@cloudinary-key",
      "CLOUDINARY_SECRET": "@cloudinary-secret",
      "PRISMIC_SECRET": "@prismic-ungaklara-secret",
      "CLOUDFLARE_ZONE": "@cloudflare-ungaklara-zone",
      "CLOUDFLARE_EMAIL": "@cloudflare-ungaklara-email",
      "CLOUDFLARE_KEY": "@cloudflare-ungaklara-key",
      "POLYFILL_FEATURES": "default-3.3,Array.prototype.find,Array.prototype.findIndex,Array.prototype.includes"
    }
  },
  "author": "code and conspire",
  "license": "CC-BY-SA-4.0",
  "bugs": {
    "url": "https://github.com/codeandconspire/ungaklara.se/issues"
  },
  "homepage": "https://github.com/codeandconspire/ungaklara.se#readme",
  "devDependencies": {
    "choo-devtools": "^2.5.1",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "cccpurge": "^2.0.0",
    "choo": "^6.13.1",
    "choo-meta": "^1.0.3",
    "choo-service-worker": "^2.4.0",
    "cloudinary": "^1.13.2",
    "date-fns": "^1.30.1",
    "dotenv": "^6.2.0",
    "focus-visible": "^4.1.5",
    "jalla": "^0.12.0-2",
    "koa-body": "^4.0.8",
    "koa-compose": "^4.1.0",
    "nanolru": "^1.0.0",
    "nanoraf": "^3.1.0",
    "normalize.css": "^8.0.1",
    "postcss-custom-properties": "^8.0.9",
    "postcss-focus-visible": "^4.0.0",
    "postcss-url": "^8.0.0",
    "prismic-element": "^1.0.0",
    "prismic-javascript": "^2.0.1",
    "prismic-richtext": "^0.4.2",
    "smoothscroll-polyfill": "^0.4.3"
  }
}
